{
    "name": "@kurlytail/gen-lib",
    "description": "Source generator library",
    "version": "0.0.1",
    "main": "dist/app.min.js",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.0",
        "chalk": "^2.4.1",
        "circular-dependency-plugin": "^4.2.1",
        "debug": "^3.1.0",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.6.0",
        "eslint-import-resolver-babel-module": "^4.0.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jest": "^21.2.0",
        "eslint-plugin-prettier": "^2.3.1",
        "exports-loader": "^0.7.0",
        "file-loader": "^1.1.6",
        "imports-loader": "^0.8.0",
        "jest": "^23.6.0",
        "jest-junit-reporter": "^1.1.0",
        "jest-localstorage-mock": "^2.2.0",
        "jsdoc": "^3.5.5",
        "prettier": "^1.10.2",
        "rewire": "^4.0.1",
        "rimraf": "^2.6.2",
        "source-map-loader": "^0.2.3",
        "source-map-support": "^0.5.0",
        "stringifier": "^1.4.0",
        "uglifyjs-webpack-plugin": "^1.0.0",
        "uuid": "^3.1.0",
        "webpack": "^3.5.5",
        "webpack-node-externals": "^1.7.2"
    },
    "scripts": {
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "build": "NODE_ENV=\"debug\" webpack --display-modules --display-reasons; chmod +x dist/sgen.min.js",
        "build-prod": "NODE_ENV=\"production\" webpack --display-modules --display-reasons; chmod +x dist/sgen.min.js",
        "lint": "eslint src",
        "test": "jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml"
    },
    "license": "UNLICENSED",
    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": [
            "node_modules",
            "./src/js"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "collectCoverageFrom": [
            "src/js/**/*.js",
            "!**/node_modules/**"
        ],
        "setupFiles": [
            "jest-localstorage-mock"
        ]
    },
    "dependencies": {
        "child_process": "^1.0.2",
        "chmod": "^0.2.1",
        "flat": "^4.1.0",
        "fs": "0.0.1-security",
        "lodash": "^4.17.11",
        "logform": "^2.0.0",
        "mkdirp": "^0.5.1",
        "node-diff3": "^1.0.0",
        "node-getopt": "^0.3.2",
        "path": "^0.12.7",
        "three-way-merge": "^0.1.0",
        "underscore": "^1.9.1",
        "webpack-permissions-plugin": "^1.0.0",
        "winston": "^3.1.0"
    },
    "bin": {
        "sgen": "dist/sgen.min.js"
    }
}
