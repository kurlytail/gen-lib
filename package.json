{
    "name": "@kurlytail/gen-lib",
    "description": "Source generator library",
    "version": "1.3.63",
    "main": "dist/lib.min.js",
    "devDependencies": {
        "@babel/cli": "7.6.4",
        "@babel/core": "7.6.4",
        "@babel/plugin-proposal-class-properties": "7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "7.6.2",
        "@babel/preset-env": "7.6.3",
        "@babel/preset-flow": "7.0.0",
        "@babel/register": "7.6.2",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.3",
        "babel-jest": "24.9.0",
        "babel-loader": "8.0.6",
        "babel-plugin-module-resolver": "3.2.0",
        "chalk": "2.4.2",
        "circular-dependency-plugin": "5.2.0",
        "debug": "4.1.1",
        "eslint": "5.16.0",
        "eslint-config-prettier": "6.5.0",
        "eslint-import-resolver-babel-module": "5.1.0",
        "eslint-loader": "3.0.2",
        "eslint-plugin-babel": "5.3.0",
        "eslint-plugin-flowtype": "4.3.0",
        "eslint-plugin-import": "2.18.2",
        "eslint-plugin-jest": "23.0.2",
        "eslint-plugin-prettier": "3.1.1",
        "exports-loader": "0.7.0",
        "file-loader": "4.2.0",
        "flow-bin": "0.111.0",
        "imports-loader": "0.8.0",
        "jest": "24.9.0",
        "jest-junit-reporter": "1.1.0",
        "jest-localstorage-mock": "2.4.0",
        "jsdoc": "3.6.3",
        "rewire": "4.0.1",
        "rimraf": "3.0.0",
        "source-map-loader": "0.2.4",
        "source-map-support": "0.5.16",
        "stringifier": "2.1.0",
        "uglifyjs-webpack-plugin": "2.2.0",
        "uuid": "3.3.3",
        "webpack": "4.41.2",
        "webpack-cli": "3.3.10",
        "webpack-node-externals": "1.7.2",
        "webpack-permissions-plugin": "1.0.6"
    },
    "scripts": {
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "build": "NODE_ENV=\"debug\" webpack --display-modules --display-reasons; chmod +x dist/sgen.min.js",
        "build-prod": "NODE_ENV=\"production\" webpack --display-modules --display-reasons; chmod +x dist/sgen.min.js",
        "lint": "eslint src",
        "test": "NODE_OPTIONS='--max_old_space_size=4096' jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml"
    },
    "license": "UNLICENSED",
    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": [
            "node_modules",
            "./src/js"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "collectCoverageFrom": [
            "src/js/**/*.js",
            "!**/node_modules/**"
        ],
        "setupFiles": [
            "jest-localstorage-mock"
        ],
        "globals": {
            "app": {
                "version": "test"
            }
        }
    },
    "dependencies": {
        "app-root-path": "2.2.1",
        "child_process": "1.0.2",
        "chmod": "0.2.1",
        "deepmerge": "^4.0.0",
        "flat": "5.0.0",
        "fs": "0.0.2",
        "lodash": "4.17.15",
        "logform": "2.1.2",
        "mkdirp": "0.5.1",
        "node-getopt": "0.3.2",
        "nodegit": "0.24.1",
        "path": "0.12.7",
        "prettier": "1.18.2",
        "prettier-plugin-java": "0.4.0",
        "underscore": "1.9.1",
        "winston": "3.2.1",
        "yaml": "1.7.2"
    },
    "bin": {
        "sgen": "dist/sgen.min.js"
    }
}
